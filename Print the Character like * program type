import java.util.*;
class C{
    public static void main(String[] args){
        Scanner s =new Scanner(System.in);
        int  a=s.nextInt();
        for(int i=1; i<=a; i++){
            for(char j='A'; j<'A'+i; j++){
               
                System.out.print(j);
            
          
            }
        
    System.out.println();
    }}
}
