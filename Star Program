
import java.util.*;
class C{
    public static void main(String[] args){
        Scanner s =new Scanner(System.in);
        int  a=s.nextInt();
        int n=1;
        for(int i=0; i<a; i++){
            for(int j=0; j<a; j++){
                if(i==j || i+j==a-1 ){
                System.out.print("*");
            
            }else{
                System.out.print(" ");
            }
            }
        
    System.out.println();
    }}
}
