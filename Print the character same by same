import java.util.*;
class C{
    public static void main(String[] args){
        Scanner s =new Scanner(System.in);
        int  a=s.nextInt();
        for(int i=1; i<=a; i++){
            char o=(char)('A'+i-1);
            for(int j=1; j<=i; j++){
               
                System.out.print(o);
            
          
            }
        
    System.out.println();
    }}
}
